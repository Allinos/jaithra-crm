<div class="main-card">
    <ul class="cards">
        <% data?.forEach((e)=>{ %>
        <li class="cards_item">
            <div class="card">
                <div class="card_image">
                    <span class="note note-left" id="refid" data-id="<%= e.id %>">#<%= e.id %></span>
                    <span class="note">
                        <%= e.category %>
                    </span>
                    <span class="note moreImg" onclick="loadGallery(<%= e.id %>)">
                        <img src="/img/img-icon.svg" class="svg" style="border-radius:0 ;" alt="">+
                    </span>
                    <img src="/uploads/<%= e.imgLink %>" />
                    <span class="card_price"><span>BHK:</span>
                        <%= e.bhk %>
                    </span>
                </div>
                <div class="card_content">
                    <h2 class="card_title">Apartment- <%= e.name %>
                    </h2>
                    <div class="card_text">
                        <!-- <hr /> -->
                        <p><strong>Location:</strong>
                            <%= e.location %>
                        </p>
                        <p><strong>Floor:</strong>
                            <%= e.floor %>
                        </p>
                        <p><strong>Owner:</strong>
                            <%= e.owner_name %>
                        </p>
                        <p><strong>Contact:</strong>
                            <%= e.owner_number %>
                        </p>
                        <% if (e.amount) { %>
                        <p><strong>Amount:</strong>
                            <%= e.amount %>
                        </p>
                        <% } %>
                        <a href="<%= e.map_link %>" target="_blank">
                            <span class="view-map pressEffect">View on Map
                                <svg width="15" height="14" xmlns="http://www.w3.org/2000/svg" version="1.1"
                                    fill="none" stroke="#000000" stroke-linecap="round" stroke-linejoin="round"
                                    stroke-width="1.5">
                                    <polyline points="8.25 2.75,2.75 2.75,2.75 13.25,13.25 13.25,13.25 7.75" />
                                    <path d="m13.25 2.75-5.5 5.5m3-6.5h3.5v3.5" />
                                </svg></span>
                        </a>
                    </div>
                    <span class="note share pressEffect" onclick="copyLink('<%= e.id %>')"><svg
                            xmlns="http://www.w3.org/2000/svg" class="svg" x="0px" y="0px" viewBox="0 0 30 30">
                            <path d="M 23 3 A 4 4 0 0 0 19 7 A 4 4 0 0 0 19.09375 7.8359375 L 10.011719 12.376953 A 4 4 0 0 0 7 11 A 4 4 0 0 0 3 15 A 4 4 0 0 0 7 19 A 4 4 0 0 0 10.013672 17.625 L 19.089844 22.164062 A 4 4 0 0 0 19 23 A 4 4 0 0 0 23 27 A 4 4 0 0 0 27 23 A 4 4 0 0 0 23 19 A 4 4 0 0 0 19.986328 20.375 L 10.910156 15.835938 A 4 4 0 0 0 11 15 A 4 4 0 0 0 10.90625 14.166016 L 19.988281 9.625 A 4 4 0 0 0 23 11 A 4 4 0 0 0 27 7 A 4 4 0 0 0 23 3 z">
                            </path>
                        </svg></span>
                </div>
            </div>
        </li>
        <% }) %>
    </ul>
</div>
